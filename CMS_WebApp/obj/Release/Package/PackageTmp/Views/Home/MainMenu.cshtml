@model List<CMS_WebApp.Areas.ad.Models.MenuModel>

<nav id="navbar" class="navbar">
    <ul>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <li><a class="nav-link scrollto" href="@item.URL">@item.Title</a></li>
            }
        }
    </ul>
    <i class="bi bi-list mobile-nav-toggle"></i>
</nav><!-- .navbar -->
